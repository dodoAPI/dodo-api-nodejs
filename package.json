{
  "name": "dodo-api",
  "version": "1.0.1",
  "description": "A dodoAPI wrapper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodoAPI/dodo-api-nodejs.git"
  },
  "keywords": [
    "dodoAPI",
    "rest",
    "api",
    "wrapper"
  ],
  "author": "dodoAPI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dodoAPI/dodo-api-nodejs/issues"
  },
  "homepage": "https://github.com/dodoAPI/dodo-api-nodejs#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.2.1"
  }
}
